# Self-Assessment: Manipulating Collections #################################

#Syahirah, 10/08/2022

tuple=(4,1,2,6,3,5,7,8,9,0,-1,-2)
list1=[]
for i in range(4):
    list1.append(tuple[i])
list1.reverse()
list2=[]
for i in range(4,8):
    list2.append(tuple[i])
list2.reverse()
list3=[]
for i in range(8,12):
    list3.append(tuple[i])
list3.reverse()
print(list1);print(list2);print(list3)
